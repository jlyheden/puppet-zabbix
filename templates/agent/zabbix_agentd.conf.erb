<%- conf_d = scope.lookupvar('zabbix::params::agent_conf_d') -%>
Server=<%= server_real %>
<%- agent_parameters_real.sort_by { |k,v| k }.each do |k| -%>
<%= k[0] %>=<%= k[1] %>
<%- end -%>
<%- if conf_d.end_with?('/') -%>
Include=<%= conf_d %>
<%- else -%>
Include=<%= "%s/" % conf_d %>
<%- end -%>
